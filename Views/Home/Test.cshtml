@{
    ViewData["Title"] = "Test";
}
@model List<IhsanWeb.ViewModel.ProductViewModel>

<table class="table table-info">
    <thead>
        <tr>
            <th>S/N</th>
            <th>Product Name</th>
            <th>Status of Test</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Description</td>
            </tr>
        }
    </tbody>
</table>